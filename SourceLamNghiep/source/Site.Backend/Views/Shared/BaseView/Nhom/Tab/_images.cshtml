@model Models.Category.CategoryModel

@if (Model.Images == null || !Model.Images.Any())
{
    <div id="divImageList" class="form-group">
        <div class="col-md-3">
            <input type="file" name="fileImage1" id="fileImage1" accept="image/*" />
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="form-group">
        <div class="col-md-12 mt10">
            <button type="button" class="btn btn-info" onclick="category.addNewImageClickOnEditCategory()">
                <i class="fa fa-plus"></i>@BackendMessage.AddNew
            </button>
        </div>
    </div>
}