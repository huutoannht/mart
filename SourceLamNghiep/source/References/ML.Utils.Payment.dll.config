<?xml version="1.0"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ServiceSoap">
          <security mode="Transport" />
        </binding>
        <binding name="ServiceSoap1" />
        <binding name="WebServicesSoap">
          <security mode="Transport" />
        </binding>
        <binding name="WebServicesSoap1" />
        <binding name="TransactionServiceSoapBinding">
          <security mode="Transport" />
        </binding>
        <binding name="TransactionServiceSoapBinding1" />
      </basicHttpBinding>
      <customBinding>
        <binding name="WebServicesSoap12">
          <textMessageEncoding messageVersion="Soap12" />
          <httpsTransport />
        </binding>
      </customBinding>
    </bindings>
    <client>
      <endpoint address="https://api.demo.globalgatewaye4.firstdata.com/transaction/v14"
        binding="basicHttpBinding" bindingConfiguration="ServiceSoap"
        contract="FirstDataAPI.ServiceSoap" name="ServiceSoap" />
      <endpoint address="https://websvctest.trans2pay.com/v3.0/Webservices.asmx"
        binding="basicHttpBinding" bindingConfiguration="WebServicesSoap"
        contract="Trans2PayAPI.WebServicesSoap" name="WebServicesSoap" />
      <endpoint address="https://websvctest.trans2pay.com/v3.0/Webservices.asmx"
        binding="customBinding" bindingConfiguration="WebServicesSoap12"
        contract="Trans2PayAPI.WebServicesSoap" name="WebServicesSoap12" />
      <endpoint address="https://trans.myprismpay.com/Web/services/TransactionService"
        binding="basicHttpBinding" bindingConfiguration="TransactionServiceSoapBinding"
        contract="PrismPayAPI.TransactionSOAPBindingImpl" name="TransactionService" />
    </client>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
