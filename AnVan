    <script type="text/javascript">
        (function() {

            if (jQuery.ajaxPrefilter) {



                var rURL = /^([^\/]+)/,

                    rEsc = /(\-|\.)/,

                    protocol = rURL.exec(document.location.href)[1].replace(rEsc, "\\$1"),

                    rDomains = {};



                jQuery.ajaxPrefilter(function(options) {

                    var strip = options.stripDomain,

                        rDomain,

                        sDomain;

                    if (strip) {

                        rDomain = rDomains[strip];

                        if (!rDomain) {

                            sDomain = "^(?:" + protocol + ")?\\/\\/" + strip.replace(rEsc, "\\$1");

                            rDomains[strip] = rDomain = new RegExp(sDomain, "i")

                        }

                        options.url = options.url.replace(rDomain, "");

                    }

                });

            }


        })(jQuery);


        jQuery.ajaxSetup({

            stripDomain: "anvan.vn"

        });




        $('.project-thummnail #menu-slider-jdock > a').each(function() {

            if ($(this).attr('href') == '') {

                if ($(this).attr('rel') == 1024180) $(this).attr('href', '/479/introduction/1052002/sariwon.html');

                if ($(this).attr('rel') == 1031125) $(this).attr('href', '/479/introduction/1068478/socialhouse.html');

            }

        });

        $('#cart-footer').hover(function() {

            $('#cart-footer').animate({

                right: '-5px'

            });

        }, function() {

            $('#cart-footer').animate({

                right: '-115px'

            });

        });
    </script>



